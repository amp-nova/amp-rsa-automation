{
  "label": "{{sfmc.label}}",
  "active": true,
  "hubId": "{{cms.hubId}}",
  "dc-credentials": {
    "id": "{{cms.clientId}}",
    "secret": "{{cms.clientSecret}}"
  },
  "sfmc-credentials": {
    "id": "{{sfmc.sfmcCredentials.id}}",
    "secret": "{{sfmc.sfmcCredentials.secret}}",
    "authDomain": "{{sfmc.sfmcCredentials.authDomain}}"
  },
  "contentAssets": {
    "topLevelFolder": "{{cms.hubLabel}}",
    "contentTypeSchemas": [
    {{#each sfmc.contentAssets.contentTypeSchemas}}
      {
        "id": "{{this.id}}",
        "htmlTemplate": "{{this.htmlTemplate}}"
      }{{#unless @last}},{{/unless}}
    {{/each}}
    ]
  },
  "rendering": {
    "vseDomain": "{{cms.stagingApi}}"
  }
}