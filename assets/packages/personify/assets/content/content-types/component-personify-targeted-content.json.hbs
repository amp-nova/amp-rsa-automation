{
  "contentTypeUri": "{{cms.contentBaseUri}}/component-personify-targeted-content.json",
  "status": "ACTIVE",
  "settings": {
    "label": "Targeted Content",
    "icons": [
      {
        "size": 256,
        "url": "https://i1.adis.ws/i/willow/CT-icon-style4-targetedcontent?fmt=jpg&bg=white&w=300"
      }
    ],
    "visualizations": [
      {
        "label": "Website",
        "templatedUri": "{{app.url}}/cms/visualization?content=\{{content.sys.id}}&vse=\{{vse.domain}}&locale=\{{locales}}",
        "default": true
      },
      {
        "label": "Development",
        "templatedUri": "http://localhost:3000/cms/visualization?content=\{{content.sys.id}}&vse=\{{vse.domain}}&locale=\{{locales}}&realtime=true",
        "default": false
      },
      {
        "label": "QR for Mobile",
        "templatedUri": "{{app.url}}/visualization/qr?vse=\{{vse.domain}}&content=\{{content.sys.id}}&locale=\{{locales}}",
        "default": false
      },
      {
        "label": "JSON",
        "templatedUri": "https://delivery-preview.visualizations.content.amplience.net?id=\{{content.sys.id}}&vse=\{{vse.domain}}&locale=\{{locales}}&hub={{cms.hubName}}&realtime=true",
        "default": false
      }
    ],
    "cards": [
      {
        "templatedUri": "{{cms.cardsAppUrl}}/?#vse=\{{vse.domain}}&content=\{{content.sys.id}}"
      }
    ]
  }
